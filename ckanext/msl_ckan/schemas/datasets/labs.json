{
  "scheming_version": 2,
  "dataset_type": "lab",
  "about_url": "http://github.com/ckan/ckanext-scheming",
  "dataset_fields": [
    {
      "field_name": "title",
      "label": "Name",
      "form_snippet": "large_text.html",
      "form_attrs": {
        "data-module": "slug-preview-target"
      },
      "form_placeholder": "lab name"
    },
    {
      "field_name": "name",
      "label": "URL",
      "form_snippet": "slug.html",
      "form_placeholder": "my-theme"
    },
    {
      "field_name": "owner_org",
      "label": "Institute",
      "preset": "dataset_organization"
    },
    {
      "field_name": "notes",
      "label": "Description",
      "form_snippet": "markdown.html",
      "form_placeholder": "A little information about my group..."
    },
    {
      "field_name": "city",
      "label": "city"
    },
    {
      "field_name": "country",
      "label": "country",
      "preset": "select",
      "choices": [
        {
          "value": "nl",
          "label": "NL"
        },
        {
          "value": "ger",
          "label": "GER"
        },
        {
          "value": "it",
          "label": "IT"
        },
        {
          "value": "sp",
          "label": "SP"
        },
        {
          "value": "por",
          "label": "POR"
        },
        {
          "value": "uk",
          "label": "UK"
        },
        {
          "value": "sw",
          "label": "SW"
        }
      ]
    },
    {
      "field_name": "equipment",
      "label": "Equipment types",
      "repeating_subfields": [
        {
          "field_name": "apparatus",
          "label": "Apparatus"
        }
      ]
    },
    {
      "field_name": "website",
      "label": "website",
      "display_snippet": "link.html",
      "form_placeholder": "resource_url_upload"
    },
    {
      "field_name": "url",
      "label": "Image URL",
      "form_placeholder": "http://example.com/my-image.jpg"
    }
  ],
  "resource_fields": [
    {
      "field_name": "url",
      "label": "URL",
      "preset": "resource_url_upload"
    },
    {
      "field_name": "name",
      "label": "Title",
      "form_placeholder": "Descriptive name of the resource."
    },
    {
      "field_name": "description",
      "label": "Description",
      "form_snippet": "markdown.html",
      "form_placeholder": "Summary explanation of file contents, purpose, origination, methods and usage guidance."
    },
    {
      "field_name": "format",
      "label": "Format",
      "preset": "resource_format_autocomplete"
    }
  ]
}